      "source": [
        "1. Create a new table from the provided table and add a movie ID"
      ],
      "metadata": {
        "id": "XRfpmyv8YrbD"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "execSQL(conn,\"drop table if exists movies\")\n",
        "\n",
        "execSQL(conn,\"CREATE TABLE temp AS SELECT DISTINCT * FROM IMDB_horror_movies\")\n",
        "q='''\n",
        "CREATE TABLE movies AS\n",
        "SELECT ROWID as movie_id, *\n",
        "  FROM temp\n",
        "'''\n",
        "execSQL(conn,q)\n",
        "execSQL(conn,\"drop table if exists temp\")\n",
        "\n",
        "#checking new table\n",
        "pd.read_sql_query(\"select * from movies\",conn)"
      ],
